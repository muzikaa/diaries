{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_server-islands\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/_image\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/404\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/register\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/signin\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/signout\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/posts\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/posts\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/edit\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/post\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/postings\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/publish\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/register\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/signin\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		},
		{
			"src": "/.*",
			"dest": "_render",
			"status": 404
		}
	]
}